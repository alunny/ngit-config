	delete-remote = !sh -c \"git push \\$0 \\:\\$1\"
	r = remote
	rv = remote -v
	aliases = !git config --get-regexp 'alias.*' | colrm 1 6 | sed 's/[ ]/ = /'
	cbt = !sh -c \"git checkout -b \\$1 -t \\$0\\/\\$1\"
	fbr = !sh -c \"git branch \\$0 origin/master\"
